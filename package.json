    "@emotion/styled": "^11.11.0",
    "@tanstack/react-query": "^4.29.19",
    "@tanstack/react-query-devtools": "^4.29.19",
    "axios": "^1.4.0",
    "framer-motion": "^10.12.18",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "^4.0.10",
    "react-hook-form": "^7.45.1",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@chakra-ui/storybook-addon": "^5.0.0",
